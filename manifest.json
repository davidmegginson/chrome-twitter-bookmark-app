{
    "manifest_version": 2,
    "name": "Twitter",
    "description": "Twitter microblogging",
    "version": "1.0",
    "icons": {
        "128": "icons/128.png"
    },
    "app": {
        "urls": [
            "https://twitter.com/"
        ],
        "launch": {
            "web_url": "https://twitter.com/"
        }
    },
    "permissions": [
        "unlimitedStorage",
        "notifications"
    ]
}
